/**app.wxss**/
page {
  background-color: #f0f0f0;
  height: 100%;
}

scroll-view {
  height: 100%;
}

.clear {
  display: table;
}

.clear::after {
  content: "";
  display: block;
  clear: both;
}

.load-more {
  line-height: 60rpx;
  font-size: 26rpx;
  text-align: center;
  color: #333;
}

.navigator-hover {
  background: transparent;
}